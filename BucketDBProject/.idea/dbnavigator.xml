CREATE TABLE "account" (
	"account_id" INT GENERATED ALWAYS AS IDENTITY,
	"buckit_user_id" INT,
	"current_balance" REAL NOT NULL,
	"account_number" TEXT NOT NULL,
	"institution_name" TEXT NOT NULL,
